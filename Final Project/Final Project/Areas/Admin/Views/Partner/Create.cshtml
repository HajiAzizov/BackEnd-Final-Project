@model PartnerCreateVM
@{
    ViewData["Title"] = "Add New Partner";
}

<div class="container mt-4">
    <h2>Add New Partner</h2>
    <form asp-action="Create" method="post" enctype="multipart/form-data" class="mt-3">
        <div class="mb-3">
            <label asp-for="ImgFile" class="form-label">Select Image</label>
            <input asp-for="ImgFile" type="file" class="form-control" />
            <span asp-validation-for="ImgFile" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-success">Create</button>
        <a asp-action="Index" class="btn btn-secondary">Back</a>
    </form>
</div>
